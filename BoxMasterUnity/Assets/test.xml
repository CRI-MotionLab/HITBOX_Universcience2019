<?xml version="1.0" encoding="utf-8"?>
<GameSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <name />
  <p1_color>#009942</p1_color>
  <p2_color>#FFD600</p2_color>
  <menu_settings>
    <timeout_screen>30</timeout_screen>
    <timeout>30</timeout>
    <timeout_menu>10</timeout_menu>
    <page_settings>
      <catchscreen_page>
        <video common="true">catch_video.mp4</video>
      </catchscreen_page>
      <content_page display_next="true" next_style="1" type="2">
        <title common="false">SCREEN1_TITLE</title>
        <content common="false">SCREEN1_CONTENT</content>
        <image common="true">test_image2.png</image>
        <video common="false" />
        <audio common="false">1A6_E1_intro-txt.wav</audio>
      </content_page>
      <mode_page display_next="false" next_style="1">
        <title common="false">SCREEN2_TITLE</title>
        <p1_picto common="true">picto1P.png</p1_picto>
        <p2_picto common="true">picto2P.png</p2_picto>
      </mode_page>
      <survey_page display_next="true" next_style="1">
        <title common="false">SCREEN3_TITLE</title>
      </survey_page>
      <content_page display_next="true" next_style="1" type="2">
        <title common="false">SCREEN4_TITLE</title>
        <content common="false">SCREEN4_CONTENT</content>
        <image common="true">test_image.png</image>
        <video common="false" />
        <audio common="false">1A6_E1_csg1.wav</audio>
      </content_page>
    </page_settings>
    <screen_settings>
      <credits>
        <title common="false" />
        <left_content>
          <text common="false" />
          <copyright common="false" />
        </left_content>
        <right_content>
          <text common="false" />
          <copyright common="false" />
        </right_content>
        <margin_text common="false" />
      </credits>
      <big_screen>
        <video common="false">big_screen_catch_video.mp4</video>
      </big_screen>
      <score_screen>
        <audio common="false">1A6_E1_Fin.wav</audio>
      </score_screen>
    </screen_settings>
    <menu_layout>
      <button_type>start</button_type>
      <button_type>sound</button_type>
      <button_type>separator</button_type>
      <button_type>copyright</button_type>
    </menu_layout>
    <survey_settings p1_title_key="P1TITLEKEY" p2_title_key="P2TITLEKEY">
      <survey_questions>
        <survey_question key="Q1">
          <answers>
            <answer action="show">Q1A1</answer>
            <answer action="end">Q1A2</answer>
          </answers>
        </survey_question>
        <survey_question key="Q2">
          <answers>
            <answer action="none">Q2A1</answer>
            <answer action="none">Q2A2</answer>
            <answer action="none">Q2A3</answer>
            <answer action="none">Q2A4</answer>
            <answer action="none">Q2A5</answer>
            <answer action="none">Q2A6</answer>
            <answer action="none">Q2A7</answer>
          </answers>
        </survey_question>
        <survey_question key="Q3">
          <answers>
            <answer action="none">Q3A1</answer>
            <answer action="none">Q3A2</answer>
            <answer action="none">Q3A3</answer>
          </answers>
        </survey_question>
        <survey_question key="Q4">
          <answers>
            <answer action="none">Q4A1</answer>
            <answer action="none">Q4A2</answer>
            <answer action="none">Q4A3</answer>
            <answer action="none">Q4A4</answer>
          </answers>
        </survey_question>
      </survey_questions>
    </survey_settings>
    <audio_volume>0.75</audio_volume>
  </menu_settings>
  <gameplay_settings>
    <game_duration>60</game_duration>
    <combo_min>1</combo_min>
    <combo_duration>10</combo_duration>
    <combo_duration_multiplier>0.8</combo_duration_multiplier>
    <combo_increment>0.2</combo_increment>
    <combo_max>10</combo_max>
    <rotation_speed>0.02</rotation_speed>
    <z_rotation_speed>0</z_rotation_speed>
    <max_angular_velocity>5</max_angular_velocity>
    <min_points>1</min_points>
    <max_points>10</max_points>
    <tolerance>0.2</tolerance>
    <target_activation_delay>1</target_activation_delay>
    <target_cooldown>1</target_cooldown>
  </gameplay_settings>
  <arduino_settings>
    <touch_surface_serial_ports>
      <port>
        <name>COM6</name>
        <baud_rate>38400</baud_rate>
        <read_timeout>400</read_timeout>
        <handshake>None</handshake>
      </port>
      <port>
        <name>COM5</name>
        <baud_rate>38400</baud_rate>
        <read_timeout>400</read_timeout>
        <handshake>None</handshake>
      </port>
    </touch_surface_serial_ports>
    <touch_surface_grid rows="24" cols="24" />
    <led_control_serial_ports>
      <port>
        <name>COM9</name>
        <baud_rate>38400</baud_rate>
        <read_timeout>400</read_timeout>
        <handshake>None</handshake>
      </port>
      <port>
        <name>COM3</name>
        <baud_rate>38400</baud_rate>
        <read_timeout>400</read_timeout>
        <handshake>None</handshake>
      </port>
    </led_control_serial_ports>
    <led_control_grid rows="30" cols="60" />
    <impact_threshold>2</impact_threshold>
    <delay_off_hit>50</delay_off_hit>
  </arduino_settings>
  <lang_app_available>
    <lang_app code="fr" name="french" color="E9600F" />
    <lang_app code="en" name="english" color="009942" />
    <lang_app code="es" name="spanish" color="0096D2" />
    <lang_app code="it" name="italian" color="000000" />
    <lang_app code="ja" name="japanese" color="000000" />
